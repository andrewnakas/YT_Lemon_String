{
  "name": "yt-lemon-string",
  "version": "1.0.0",
  "description": "YouTube-based music player with Spotify-like interface - Web and Desktop",
  "scripts": {
    "install:all": "cd backend && npm install && cd ../electron && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:electron": "cd electron && npm start",
    "build:mac": "cd electron && npm run build:mac",
    "build:win": "cd electron && npm run build:win",
    "build:linux": "cd electron && npm run build:linux",
    "build:all": "cd electron && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewnakas/YT_Lemon_String.git"
  },
  "keywords": [
    "youtube",
    "music",
    "player",
    "electron",
    "desktop",
    "spotify"
  ],
